extends c

# for size_t
snippet fors
for (size_t ${1} = 0; $1 < ${2:count}; $1++) {
	${3};
}
endsnippet

# for double
snippet ford
for (size_t ${1} = 0; $1 < ${2:count}; $1++) {
	for (size_t ${3} = 0; $3 < ${4:count2}; $3++) {
		${4};
	}
}
endsnippet

# foreach
snippet fore
for (auto &${1} : ${2:container}){
	${3};
}
endsnippet

